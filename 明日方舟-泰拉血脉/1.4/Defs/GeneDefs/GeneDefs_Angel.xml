<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef ParentName="GeneHeadboneBase" Name="Ark_AngelHaloBase" Abstract="True">
		<description>Carriers of this gene will generate a disembodied floating halo above their head, which can enhance the carrier's psychic sensitivity.</description>
		<prerequisite>Ark_Sectator</prerequisite>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<displayCategory>Ark_Angel</displayCategory>
		<ignoreDarkness>true</ignoreDarkness>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>sacred</symbol>
				</li>
				<li>
					<symbol>bright</symbol>
				</li>
				<li>
					<symbol>dazzling</symbol>
				</li>
				<li>
					<symbol>decorative</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/Ark_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/Ark_Xenogene</backgroundPathXenogenes>
				<hediffsToBodyParts>
					<li>
						<hediff>Ark_hediff_Halo</hediff>
						<bodyparts>
							<li>Head</li>
						</bodyparts>
					</li>
				</hediffsToBodyParts>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="Ark_WingBase" Name="Ark_AngelWingBase" Abstract="True">
		<description>The carrier of this gene will generate disembodied light wings on his back. The light wings do not have the function of flying but can effectively improve the carrier's maneuverability.</description>
		<prerequisite>Ark_Sectator</prerequisite>
		<biostatCpx>1</biostatCpx>
		<biostatMet>-1</biostatMet>
		<displayCategory>Ark_Angel</displayCategory>
		<statOffsets>
			<MoveSpeed>0.1</MoveSpeed>
			<MeleeDodgeChance>5</MeleeDodgeChance>
		</statOffsets>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>sacred</symbol>
				</li>
				<li>
					<symbol>bright</symbol>
				</li>
				<li>
					<symbol>dazzling</symbol>
				</li>
				<li>
					<symbol>decorative</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/Ark_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/Ark_Xenogene</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	
	<GeneDef ParentName="Ark_AngelWingBase">
		<defName>Ark_LightWings1</defName>
		<label>Overloaded Light Wings</label>
		<iconPath>Things/Pawn/Humanlike/BodyAttachments/Ark_LightWings/Ark_Wings1_south</iconPath>
