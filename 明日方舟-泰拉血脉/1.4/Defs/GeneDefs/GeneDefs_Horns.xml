<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<GeneDef Name="Ark_HeadboneGeneBase" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<biostatMet>0</biostatMet>
		<randomChosen>true</randomChosen>
		<endogeneCategory>Headbone</endogeneCategory>
		<displayCategory>Ark_Heads</displayCategory>
		<displayOrderInCategory>50</displayOrderInCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<graphicData>
			<layer>PostHair</layer>
			<useSkinShader>false</useSkinShader>
			<drawLoc>HeadTop</drawLoc>
		</graphicData>
		<exclusionTags>
			<li>Ark_Headbone</li>
			<li>Headbone</li>
		</exclusionTags>
	</GeneDef>

	<!--Waifan's Corner-->
	<GeneDef ParentName="Ark_HeadboneGeneBase">
		<defName>Ark_VouivreHorn1</defName>
		<label>Cyan Dragon Horns</label>
		<description>Carriers of this gene will develop upward-curving cyan horns on both sides of their foreheads.</description>
		<iconPath>UI/Icons/Genes/Ark_VouivreHorn1</iconPath>
		<prerequisite>Ark_Vouivre</prerequisite>
		<graphicData>
			<drawScale>2</drawScale>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_VouivreHorn/Ark_VouivreHorn1</graphicPath>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>锐利</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>角</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/Ark_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/Ark_Xenogene</backgroundPathXenogenes>
				<hediffsToBodyParts>
					<li>
						<hediff>Ark_HornAttack</hediff>
						<bodyparts>
							<li>Head</li>
						</bodyparts>
					</li>
				</hediffsToBodyParts>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="Ark_HeadboneGeneBase">
		<defName>Ark_VouivreHorn2</defName>
		<label>Black Iron Dragon Horns</label>
		<description>Carriers of the gene develop gray, downward-curving horns on both sides of their foreheads.</description>
		<iconPath>UI/Icons/Genes/Ark_VouivreHorn2</iconPath>
		<prerequisite>Ark_Vouivre</prerequisite>
		<graphicData>
			<drawScale>2</drawScale>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_VouivreHorn/Ark_VouivreHorn2</graphicPath>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>Hard</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>Horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/Ark_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/Ark_Xenogene</backgroundPathXenogenes>
				<hediffsToBodyParts>
					<li>
						<hediff>Ark_HornAttack2</hediff>
						<bodyparts>
							<li>Head</li>
						</bodyparts>
					</li>
				</hediffsToBodyParts>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="Ark_HeadboneGeneBase">
		<defName>Ark_VouivreHorn3</defName>
		<label>Small Black Horns</label>
		<description>Carriers of this gene will develop small black spikes on both sides of their foreheads.</description>
		<iconPath>UI/Icons/Genes/Ark_VouivreHorn3</iconPath>
		<prerequisite>Ark_Vouivre</prerequisite>
		<graphicData>
			<drawScale>2</drawScale>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_VouivreHorn/Ark_VouivreHorn3</graphicPath>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>Compact</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>Horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
			<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/Ark_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/Ark_Xenogene</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>

	<!--Oni's corner-->
	<GeneDef Name="Ark_OniHornBase" ParentName="Ark_HeadboneGeneBase" Abstract="True">
		<prerequisite>Ark_Oni</prerequisite>
		<displayCategory>Ark_OniHorn</displayCategory>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/Ark_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/Ark_Xenogene</backgroundPathXenogenes>
				<hediffsToBodyParts>
					<li>
						<hediff>Ark_HornAttack</hediff>
						<bodyparts>
							<li>Head</li>
						</bodyparts>
					</li>
				</hediffsToBodyParts>
			</li>
		</modExtensions>
	</GeneDef>
	
	<GeneDef ParentName="Ark_OniHornBase">
		<defName>Ark_OniHorn1</defName>
		<label>Night Blade Oni Horns</label>
		<description>Carriers of this gene will have a pair of short blade-like pitch black horns growing on their heads.</description>
		<iconPath>UI/Icons/Genes/Ark_OniHorn1</iconPath>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_OniHorn/Ark_OniHorn1</graphicPath>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>pitch black</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>Horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef ParentName="Ark_OniHornBase">
		<defName>Ark_OniHorn2</defName>
		<label>Single Black Oni Horn</label>
		<description>Carriers of this gene will have a standing black horn growing from their heads.</description>
		<iconPath>UI/Icons/Genes/Ark_OniHorn2</iconPath>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_OniHorn/Ark_OniHorn2</graphicPath>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>Pitch Black</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>Horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef ParentName="Ark_HeadboneGeneBase">
		<defName>Ark_OniHorn3</defName>
		<label>Rocky Oni Horns</label>
		<description>Carriers of this gene will have two pairs of horns on their heads that look like rocks, one large and one small.</description>
		<iconPath>UI/Icons/Genes/Ark_OniHorn3</iconPath>
		<prerequisite>Ark_Oni</prerequisite>
		<displayCategory>Ark_OniHorn</displayCategory>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_OniHorn/Ark_OniHorn3</graphicPath>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>Stone</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>Horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/Ark_Endogene</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/Ark_Xenogene</backgroundPathXenogenes>
				<hediffsToBodyParts>
					<li>
						<hediff>Ark_HornAttack2</hediff>
						<bodyparts>
							<li>Head</li>
						</bodyparts>
					</li>
				</hediffsToBodyParts>
			</li>
		</modExtensions>
	</GeneDef>

	<GeneDef ParentName="Ark_OniHornBase">
		<defName>Ark_OniHornBlue</defName>
		<label>Blue Oni Horns</label>
		<description>Carriers of this gene will grow a pair of curved blue horns on their foreheads.</description>
		<iconPath>UI/Icons/Genes/Ark_OniHornBlue</iconPath>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_OniHorn/Ark_OniHornBlue</graphicPath>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>Blue</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>Horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef ParentName="Ark_OniHornBase">
		<defName>Ark_OniHornRed</defName>
		<label>Red Oni Horns</label>
		<description>Carriers of this gene will grow a pair of curved, fiery red horns on their foreheads.</description>
		<iconPath>UI/Icons/Genes/Ark_OniHornRed</iconPath>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_OniHorn/Ark_OniHornRed</graphicPath>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>Fiery Red</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>Horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef ParentName="Ark_OniHornBase">
		<defName>Ark_OniHornFloating</defName>
		<label>Pigmented Oni Horns</label>
		<description>Carriers of this gene will grow a pair of curved horns on their foreheads, and the horns will show a variety of colors affected by skin pigments.</description>
		<iconPath>UI/Icons/Genes/Ark_OniHornFloating</iconPath>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_OniHorn/Ark_OniHornFloating</graphicPath>
			<colorType>Skin</colorType>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>Variety</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>Horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>

	<GeneDef ParentName="Ark_OniHornBase">
		<defName>Ark_OniHornFloating2</defName>
		<label>Single Pigmented Oni Horn</label>
		<description>Carriers of this gene will grow a single horn on their forehead, and the horn will appear in a variety of colors affected by skin pigments.</description>
		<iconPath>UI/Icons/Genes/Ark_OniHornFloating2</iconPath>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/Ark_OniHorn/Ark_OniHornFloating2</graphicPath>
			<colorType>Skin</colorType>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>Variety</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>Horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>
</Defs>